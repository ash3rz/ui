.appHyperlinkName {
    cursor: pointer;
    text-decoration: none;
    color: black;
    font-size: 12px;
    height: 15px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.appHyperlinkName:hover {
    text-decoration: underline;
}

.appName {
    text-decoration: none;
    color: black;
    font-size: 12px;
    height: 15px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.appDisabled {
    cursor: hand;
    font-size: 12px;
    height: 15px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.appDisabled:hover {
    cursor: hand;
}

