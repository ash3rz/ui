    </replace-with>

    <!-- Tool Requests -->
    <replace-with class="org.iplantc.de.theme.base.client.admin.toolRequest.ToolRequestViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.toolRequest.ToolRequestView.ToolRequestViewAppearance" />
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.admin.toolRequest.presenter.ToolRequestPresenterDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.toolRequest.ToolRequestView.Presenter.ToolRequestPresenterAppearance" />
    </replace-with>

    <!--Tool Admin-->
    <replace-with class="org.iplantc.de.theme.base.client.admin.toolAdmin.ToolAdminViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.toolAdmin.ToolAdminView.ToolAdminViewAppearance" />
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.admin.toolAdmin.cells.ToolAdminViewCellDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.toolAdmin.view.cells.ToolAdminNameCell.ToolAdminNameCellAppearance" />
    </replace-with>

    <!--Ontologies-->
    <replace-with class="org.iplantc.de.theme.base.client.admin.ontologies.OntologiesViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.ontologies.OntologiesView.OntologiesViewAppearance" />
    </replace-with>

    <!--Communities-->
    <replace-with class="org.iplantc.de.theme.base.client.admin.communities.CommunitiesViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.communities.AdminCommunitiesView.Appearance" />
    </replace-with>

    
    <!--  Metadata -->
     <replace-with class="org.iplantc.de.theme.base.client.admin.metadata.DefaultTemplateListingAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.metadata.view.TemplateListingView.TemplateListingAppearance" />
    </replace-with>
     <replace-with class="org.iplantc.de.theme.base.client.admin.metadata.DefaultMetadataPresenterAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.metadata.view.TemplateListingView.Presenter.MetadataPresenterAppearance" />
    </replace-with>

    <replace-with class="org.iplantc.de.theme.base.client.admin.permIdRequest.PermanentIdRequestViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.permIdRequest.views.PermanentIdRequestView.PermanentIdRequestViewAppearance"></when-type-is>
    </replace-with>
    
     <replace-with class="org.iplantc.de.theme.base.client.admin.permIdRequest.PermanentIdRequestPresenterDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.permIdRequest.views.PermanentIdRequestView.PermanentIdRequestPresenterAppearance"></when-type-is>
    </replace-with>

    <!-- Workshop Admin -->
    <replace-with class="org.iplantc.de.theme.base.client.admin.workshopAdmin.WorkshopAdminViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.workshopAdmin.WorkshopAdminView.WorkshopAdminViewAppearance" />
    </replace-with>
