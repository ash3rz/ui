<module>
    <inherits name="org.iplantc.de.admin.desktop.AdminDesktop" />
    <inherits name="org.iplantc.de.theme.base.client.admin.apps.AdminApps" />

    <!-- Desktop -->
    <replace-with class="org.iplantc.de.theme.base.client.admin.desktop.BelphegorViewDefaultAppearance">
        <when-type-is class="org.iplantc.de.admin.desktop.client.views.BelphegorView.BelphegorViewAppearance" />
    </replace-with>

    <!-- RefGenome -->
    <replace-with class="org.iplantc.de.theme.base.client.admin.refGenome.RefGenomeDefaultAppearance">
